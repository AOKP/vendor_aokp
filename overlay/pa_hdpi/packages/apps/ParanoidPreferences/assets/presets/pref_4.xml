<property name="Tablet UI" active="true">

        <description><![CDATA[Hybrid mode <b><font color='green'>on</font></b><br\>Tablet workspace<br\>5 Icons<br\>No softkeys]]></description>
        <setting type="Hybrid" target="%hybrid_mode" value="1"/>
        <setting type="Hybrid" target="com.android.systemui.dpi" value="160"/>
        <setting type="Hybrid" target="com.android.systemui.layout" value="720"/>
        <setting type="Android" target="max_notification_icons" value="5"/>
        <setting type="Android" target="nav_bar_status" value="0"/>

</property>
